{
  "name": "@moar-things/nend",
  "version": "0.0.1",
  "description": "Non-Editable Note Book",
  "license": "MIT",
  "author": "Patrick Mueller <pmuellr@apache.org> (https://github.com/pmuellr)",
  "homepage": "https://github.com/moar-things/nend",
  "main": "lib/nend.js",
  "bin": {
    "nend": "./lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/moar-things/nend.git"
  },
  "bugs": {
    "url": "https://github.com/moar-things/nend/issues"
  },
  "scripts": {
    "try": "node lib/nend experiments/temperature.nend.md",
    "standard": "standard -v",
    "test": "standard -v && npm -s run utest",
    "utest": "node test | tap-spec",
    "watch": "nodemon --exec 'npm -s run try; date'"
  },
  "standard": {
    "ignore": [
      "/experiments/",
      "/node_modules/",
      "/tmp/"
    ]
  },
  "dependencies": {
    "marked": "~0.3.17",
    "minimist": "~1.2.0",
    "toml": "~2.3.3"
  },
  "devDependencies": {
    "nodemon": "~1.17.2",
    "standard": "~11.0.1",
    "tap-spec": "~4.1.1",
    "tape": "~4.7.0"
  }
}
